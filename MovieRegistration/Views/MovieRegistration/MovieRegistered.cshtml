
@{
    ViewData["Title"] = "MovieRegistered";
}

@model Movie
<h3>Thank You For Registering a New Movie</h3><br />
<h4>@Model.Title has been registered! </h4><br />

<h5>Summary</h5>
<p>Title: @Model.Title</p>
<p>Genre: @Model.Genre</p>
<p>Year: @Model.Year</p>
<p>Actors: @Model.Actors</p>
<p>Directors: @Model.Directors</p>


